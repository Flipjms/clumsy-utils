{
    "name": "clumsy/utils",
    "description": "A set of utilities for building Laravel applications",
    "keywords": ["laravel", "helper", "utilities"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Buteler",
            "email": "tbuteler@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "clumsy/assets": "dev-master",
        "geoip2/geoip2":"~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-0": {
            "Clumsy\\Utils": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.7.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
